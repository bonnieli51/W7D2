
<h1> Sign Up </h1>

<form action="<%= users_url %>" method="post">
    <%= auth_token %>
    <label for="">Email:
        <input type="email" name="user[email]">
    </label>
    <br>
    <label for="">Password:
        <input type="password" name="user[password]">
    </label>
    <br>
    <input type="submit" value="Sign Up">
</form>