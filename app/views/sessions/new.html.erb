
<h1> Log In </h1>

<form action="<%= session_url %>" method="post">
    <%= auth_token %>
    <label for="">Email:
        <input type="email" name="user[email]">
    </label>
    <br>
    <label for="">Password:
        <input type="password" name="user[password]">
    </label>
    <br>
    <input type="submit" value="Log In">
</form>